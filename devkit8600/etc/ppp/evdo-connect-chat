#/etc/ppp/evdo-connect-chat
# chat script for China Telecom, used ETCOM CDMA module.
TIMEOUT         15
ABORT           "DELAYED"
ABORT           "BUSY"
ABORT           "ERROR"
ABORT           "NO DIALTONE"
ABORT           "NO CARRIER"
""              \rAT
"OK"		ATE0V1
"OK"		AT
"OK"		ATS0=0
"OK"		AT\^PREFMODE=8
"OK"		ATDT#777
CONNECT		""

